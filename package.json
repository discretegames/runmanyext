{
	"name": "runmany",
	"displayName": "RunMany",
	"description": "Syntax highlighting for RunMany, a tool that can run multiple programming languages from one file.",
	"version": "0.0.5",
	"author": {
		"name": "discretegames",
		"url": "https://github.com/discretegames",
		"email": "discretizedgames@gmail.com"
	},
	"homepage": "https://marketplace.visualstudio.com/items?itemName=discretegames.runmany",
	"publisher": "discretegames",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/discretegames/runmanyext"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#dadada"
	},
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Programming Languages",
		"Education"
	],
	"keywords": [
		"many languages",
		"one file",
		"translation",
		"chrestomathy",
		"polyglot"
	],
	"dependencies": {},
	"devDependencies": {},
	"main": "./out/extension.js",
	"activationEvents": [
		"onDebug"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"menus": {},
		"languages": [
			{
				"id": "many",
				"aliases": [
					"RunMany",
					"many"
				],
				"extensions": [
					".many",
					".runmany"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "many",
				"scopeName": "source.many",
				"path": "./syntaxes/many.tmLanguage.json",
				"embeddedLanguages": {}
			}
		],
		"debuggers": []
	}
}
