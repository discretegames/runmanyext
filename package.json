{
	"name": "runmany",
	"displayName": "RunMany",
	"description": "Syntax highlighting for the RunMany Python package that can run multiple programming languages from one file.",
	"version": "0.0.4",
	"author": {
		"name": "discretegames",
		"url": "https://github.com/discretegames",
		"email": "discretizedgames@gmail.com"
	},
	"publisher": "discretegames",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/discretegames/runmanyext"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#dadada"
	},
	"engines": {
		"vscode": "^1.45.1"
	},
	"categories": [
		"Programming Languages",
		"Education"
	],
	"keywords": [
		"many languages",
		"one file",
		"translation",
		"chrestomathy",
		"polyglot"
	],
	"contributes": {
		"languages": [
			{
				"id": "many",
				"aliases": [
					"RunMany",
					"many"
				],
				"extensions": [
					".many",
					".runmany"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "many",
				"scopeName": "source.many",
				"path": "./syntaxes/many.tmLanguage.json",
				"embeddedLanguages": {}
			}
		]
	},
	"devDependencies": {},
	"dependencies": {}
}
