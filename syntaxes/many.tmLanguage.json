{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.many",
	"name": "RunMany",
	"patterns": [{ "include": "#comments" }, { "include": "#exit" }],
	"repository": {
		"comments": {
			"patterns": [
				{
					"begin": "^/%",
					"end": "^%/\\s*$\\n?",
					"name": "comment.many"
				},
				{
					"match": "^(%).*$\\n?",
					"name": "comment.many"
				}
			]
		},
		"exit": {
			"patterns": [
				{
					"match": "^Exit\\.\\s*$\\n?",
					"name": "keyword.exit.many"
				}
			]
		}
	}
}
