{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.many",
	"name": "RunMany",
	"patterns": [{ "include": "#block-comment" }, { "include": "#inline-comment" }, { "include": "#exit" }],
	"repository": {
		"block-comment": {
			"applyEndPatternLast": 1,
			"begin": "^/%",
			"end": "^%/\\s*$\\n?",
			"name": "comment.block.many",
			"patterns": [
				{
					"include": "#block-comment"
				}
			]
		},
		"inline-comment": {
			"match": "^(%).*$\\n?",
			"name": "comment.line.many"
		},
		"exit": {
			"match": "^Exit\\.\\s*$\\n?",
			"name": "keyword.exit.many"
		}
	}
}
