{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.many",
	"name": "RunMany",
	"patterns": [
		{
			"include": "#block-comment"
		},
		{
			"include": "#inline-comment"
		},
		{
			"include": "#exit"
		},
		{
			"include": "#argv-section"
		},
		{
			"include": "#invalid-header"
		},
		{
			"include": "#disabled-section"
		}
	],
	"repository": {
		"block-comment": {
			"name": "comment.block.many",
			"applyEndPatternLast": 1,
			"begin": "^/%",
			"end": "^%/\\s*$",
			"patterns": [
				{
					"include": "#block-comment"
				}
			]
		},
		"inline-comment": {
			"name": "comment.line.many",
			"match": "^(%).*$\\n?"
		},
		"exit": {
			"name": "keyword.exit.many",
			"match": "^Exit\\.\\s*$ "
		},
		"argv-section": {
			"name": "keyword.argv.section.many",
			"begin": "^Argv\\s*:",
			"while": "idk"
		},
		"invalid-header": {
			"patterns": [
				{ "name": "invalid.badly-indented.header.section.many", "match": "^(?!\\s*$) {1,3}[^ ].*$" },
				{ "name": "invalid.unrecognized.header.section.many", "match": "^(?!\\s*$| {4}|\t)(?!.*:)[^ ].*$" }
			]
		},
		"disabled-section": {
			"name": "comment.disabled.section.many",
			"begin": "^![^:]*:",
			"while": "^!?Also\\s*:"
		}
	}
}
