{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.many",
	"name": "RunMany",
	"patterns": [{ "include": "#comments" }],
	"repository": {
		"comments": {
			"patterns": [
				{
					"begin": "^/%",
					"captures": {
						"0": {
							"name": "punctuation.definition.comment.many.comments"
						}
					},
					"end": "^%/\\s*$",
					"name": "comment.block.many.comments"
				},
				{
					"captures": {
						"1": {
							"name": "punctuation.definition.comment.many.comments"
						}
					},
					"match": "^(%).*$\\n?",
					"name": "comment.line.percent-sign.many"
				}
			]
		}
	}
}
