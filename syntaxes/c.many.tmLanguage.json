{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.many",
	"name": "RunMany",
	"fileTypes": [".many"],
	"patterns": [
		{
			"include": "#ctest"
		}
	],
	"repository": {
		"ctest": {
			"name": "meta.embedded.block.c",
			"xbegin": "^FOO",
			"begin": "^(?=//)",
			"xend": "^BAR",
			"xwhile": "^(?=\\t|\\s*$)",
			"while": "",
			"captures": { "0": { "name": "keyword.language.ctest" } },
			"patterns": [{ "include": "source.c" }]
		}
	}
}
