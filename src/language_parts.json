{
	"<ID>-section-list": {
		"name": "meta.section-list.code.<ID>.many",
		"begin": "(?i)^(?=<NAME>\\s*[,:])([^:]*)(:)",
		"beginCaptures": {
			"1": { "patterns": [{ "include": "#language-list" }] },
			"2": { "name": "punctuation.definition.header.end.many" }
		},
		"while": "^(?=%| {4}|\\t|\\s*$|(?:!\\s*)?Also\\s*:)",
		"patterns": [
			{ "include": "#disabled-also-section" },
			{ "include": "#also-header" },
			{ "include": "#<ID>-block" }
		]
	},
	"<ID>-block": {
		"name": "meta.embedded.block.<ID>",
		"begin": "",
		"while": "(^%.*$)|^(?= {4}|\\t|\\s*$)",
		"whileCaptures": { "1": { "patterns": [{ "include": "#comment" }] } },
		"patterns": [{ "include": "source.<SOURCE>" }]
	}
}
