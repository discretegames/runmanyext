{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.many",
	"name": "RunMany",
	"fileTypes": [".many", ".runmany"],
	"patterns": [
		{ "include": "#start" },
		{ "include": "#stop" },
		{ "include": "#comment" },
		{ "include": "#section" },
		{ "include": "invalid-headerTODO" }
	],
	"repository": {
		"start": {
			"name": "meta.start.many",
			"match": "^(START)\\s*(:)\\s*$",
			"captures": {
				"1": { "name": "keyword.control.start.many" },
				"2": { "name": "punctuation.definition.start.many" }
			}
		},
		"stop": {
			"name": "meta.stop.many",
			"contentName": "comment.block.stop.many",
			"begin": "^(STOP)\\s*(\\.)\\s*$",
			"beginCaptures": {
				"1": { "name": "keyword.control.stop.many" },
				"2": { "name": "punctuation.definition.stop.many" }
			},
			"while": ""
		},
		"comment": {
			"name": "comment.line.many",
			"match": "^\\s*(%%).*$",
			"captures": { "1": { "name": "punctuation.definition.comment.many" } }
		},
		"invalid-headerTODO": {
			"name": "invalid.illegal.unrecognized-header.many",
			"match": "^(?: {1,3}\\t?(?!%%%))?\\S.*$"
		},
		"section": {
			"name": "meta.section.many",
			"begin": "^(?=\\S)(?=(?!(?:(?:!!|@@)\\s*)?(?:[!@]\\s*)?Also\\s*:)[^:]*:)",
			"while": "^(?=\\s*%%.*$| {4}|\\t|\\s*$|(?:[!@]\\s*)?Also\\s*:|End\\s*\\.\\s*$)",
			"patterns": [{ "include": "#disabled-section" }, { "include": "#enabled-section" }]
		},
		"disabled-section": {
			"name": "string.regexp.disabled-section.many",
			"begin": "^(!!)",
			"while": "(^\\s*%%.*$)?",
			"whileCaptures": { "1": { "patterns": [{ "include": "#comment" }] } },
			"beginCaptures": { "1": { "name": "punctuation.definition.disabled-section.many" } },
			"patterns": [{ "include": "#disabled-end" }]
		},
		"disabled-end": {
			"name": "meta.disabled-end.many",
			"contentName": "invalid.illegal.section-after-end.many",
			"begin": "^End\\s*\\.",
			"while": ""
		},
		"enabled-section": {
			"name": "meta.enabled-section.many",
			"begin": "^(?:(@@)\\s*)?",
			"while": "",
			"beginCaptures": { "1": { "name": "constant.numeric.punctuation.definition.solo-section.many" } },
			"patterns": [
				{ "include": "#enabled-end" },
				{ "include": "#settings-section" },
				{ "include": "#input-section" }
			]
		},
		"enabled-end": {
			"name": "meta.enabled-end.many",
			"contentName": "invalid.illegal.section-after-end.many",
			"begin": "^(End)\\s*(\\.)",
			"while": "",
			"beginCaptures": {
				"1": { "name": "keyword.section.end.many" },
				"2": { "name": "punctuation.definition.end.many" }
			}
		},
		"disabled-snippet": {
			"name": "string.regexp.disabled-snippet.many",
			"begin": "\\G(!)",
			"while": "(^\\s*%%.*$)|^(?!(?:[!@]\\s*)?Also\\s*:)",
			"whileCaptures": { "1": { "patterns": [{ "include": "#comment" }] } },
			"beginCaptures": { "1": { "name": "punctuation.definition.disabled-snippet.many" } }
		},
		"settings-section": {
			"name": "meta.settings-section.many",
			"begin": "\\G(?=(?:[!@]\\s*)?Settings\\s*:)",
			"while": "^(?!End\\s*\\.)",
			"patterns": [{ "include": "#disabled-snippet" }, { "include": "#settings-snippet" }]
		},
		"settings-snippet": {
			"name": "meta.settings-snippet.many",
			"begin": "\\G(?:(@)\\s*)?(?:(Also)\\s*|(Settings)\\s*)(:)",
			"while": "(^\\s*%%.*$)|^(?!(?:[!@]\\s*)?Also\\s*:)",
			"whileCaptures": { "1": { "patterns": [{ "include": "#comment" }] } },
			"beginCaptures": {
				"1": { "name": "support.function.punctuation.definition.solo-snippet.many" },
				"2": { "name": "keyword.section.also.many" },
				"3": { "name": "keyword.section.settings.many" },
				"4": { "name": "punctuation.definition.header-end.many" }
			},
			"contentName": "meta.settings-snippet.content.many",
			"patterns": [{ "include": "source.json" }]
		},
		"input-section": {
			"name": "meta.input-section.many",
			"begin": "\\G(?=(?:[!@]\\s*)?(?:Argv|Stdin)\\s*(?:\\bfor(?=[\\s:])[^:]*)?:)",
			"while": "^(?!End\\s*\\.)",
			"patterns": [{ "include": "#disabled-snippet" }, { "include": "#input-snippet" }]
		},
		"input-snippet": {
			"name": "meta.input-snippet.many",
			"begin": "\\G(?:(@)\\s*)?(?:(Also)\\s*|(?:(Argv)|(Stdin))\\s*(?:\\b(for)(?=[\\s:])([^:]*))?)(:)",
			"while": "(^\\s*%%.*$)|^(?!(?:[!@]\\s*)?Also\\s*:)",
			"whileCaptures": { "1": { "patterns": [{ "include": "#comment" }] } },
			"beginCaptures": {
				"1": { "name": "support.function.punctuation.definition.solo-snippet.many" },
				"2": { "name": "keyword.section.also.many" },
				"3": { "name": "keyword.section.argv.many" },
				"4": { "name": "keyword.section.stdin.many" },
				"5": { "name": "keyword.section.for.many" },
				"6": { "patterns": [{ "include": "#input-languages" }] },
				"7": { "name": "punctuation.definition.header-end.many" }
			},
			"contentName": "string.input-snippet.content.many"
		},
		"input-languages": {
			"name": "meta.input-languages.many",
			"patterns": [
				{ "name": "variable.other.input-languages.many", "match": "[^,:]+" },
				{ "name": "punctuation.separator.input-languages.many", "match": "," }
			]
		},

		"languages": {
			"name": "meta.language-list.many",
			"patterns": [
				{ "name": "entity.name.type.language-name.many", "match": "[^,:]+" },
				{ "name": "punctuation.separator.language-list.many", "match": "," }
			]
		},

		"stdin-section-list": {
			"name": "meta.section-list.input.stdin.many",
			"begin": "^(Stdin)\\s*(?:\\b(for)\\b([^:]*))?(:)",
			"beginCaptures": {
				"1": { "name": "storage.section.stdin.many" },
				"2": { "name": "storage.section.stdin-for.many" },
				"3": { "patterns": [{ "include": "#input-language-list" }] },
				"4": { "name": "punctuation.definition.header.end.many" }
			},
			"while": "^(?=%| {4}|\\t|\\s*$|(?:!\\s*)?Also\\s*:)",
			"patterns": [
				{ "include": "#disabled-also-section" },
				{ "include": "#input-also-header" },
				{ "include": "#stdin-block" }
			]
		},
		"stdin-block": {
			"name": "string.unquoted.block.stdin.many",
			"begin": "",
			"while": "(^%.*$)|^(?= {4}|\\t|\\s*$)",
			"whileCaptures": { "1": { "patterns": [{ "include": "#leading-comment" }] } }
		},

		"unknown-section-list": {
			"name": "meta.section-list.code.unknown.many",
			"begin": "^(?=\\S)(?!Also)([^:]*)(:)",
			"beginCaptures": {
				"1": { "patterns": [{ "include": "#language-list" }] },
				"2": { "name": "punctuation.definition.header.end.many" }
			},
			"while": "^(?=%| {4}|\\t|\\s*$|(?:!\\s*)?Also\\s*:)",
			"patterns": [
				{ "include": "#disabled-also-section" },
				{ "include": "#also-header" },
				{ "include": "#unknown-block" }
			]
		},
		"unknown-block": {
			"name": "meta.embedded.block.unknown",
			"begin": "",
			"while": "(^%.*$)|^(?= {4}|\\t|\\s*$)",
			"whileCaptures": { "1": { "patterns": [{ "include": "#leading-comment" }] } }
		},

		"python-section-list": {
			"name": "meta.section-list.code.python.many",
			"begin": "(?i)^(?=Python\\s*[,:])([^:]*)(:)",
			"beginCaptures": {
				"1": {
					"patterns": [
						{
							"include": "#language-list"
						}
					]
				},
				"2": {
					"name": "punctuation.definition.header.end.many"
				}
			},
			"while": "^(?=%| {4}|\\t|\\s*$|(?:!\\s*)?Also\\s*:)",
			"patterns": [
				{
					"include": "#disabled-also-section"
				},
				{
					"include": "#also-header"
				},
				{
					"include": "#python-block"
				}
			]
		},
		"python-block": {
			"name": "meta.embedded.block.python",
			"begin": "",
			"while": "(^%.*$)|^(?=.*(%%%.*$))|^(?= {4}|\\t|\\s*$)",
			"whileCaptures": {
				"1": {
					"patterns": [
						{
							"include": "#leading-comment"
						}
					]
				}
			},
			"patterns": [
				{
					"include": "#inline-comment"
				},
				{
					"include": "source.python"
				}
			]
		}
	}
}
